#include <stdio.h>
#include <stdlib.h>
/*Un programa que pida al usuario 5 n√∫meros y luego los muestre en el orden 
contrario al que se introdujeron.
*/
int main(){
    double num[5];
    int i;
    for(i = 0; i < 5; i++){
        printf("Ingrese el valor que desea del numero %d: ", i);
        scanf("%lf", &num[i]);
    }
    for(i = 4; i >= 0; i--){
        printf("\nSus valores en orden contrario fueron: %.2f", num[i]);
    }
    
}
